public class PageNumber {
    private int BookNo;
    private int PageNo;

    public PageNumber(int b, int p) {
	BookNo = b;
	PageNo = p;
    }

    public int book() {
	return BookNo;
    }

    public int page() {
	return PageNo;
    }

    public String toString() {
	StringBuilder page = new StringBuilder();

	page.append(BookNo);
	page.append("-");
	page.append(PageNo);

	return page.toString();
    }
}
